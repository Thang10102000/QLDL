        <scalar-parameter name="strFromDate" id="527">
            <method name="validate"><![CDATA[//import package
importPackage(Packages.java.lang);
// lay ve ngay hien tai
var currentDate=new Date();
//cong them 31 ngay vao gia tri param fromDate
var tmpFromDate=BirtDateTime.addDay( params["strFromDate"], 30 )
 
if(params["strFromDate"].compareTo(currentDate)>0){
// thong bao loi
    throw  new Exception("Từ ngày phải nhỏ hơn hoặc bằng ngày hiện tại");
}
 
if(tmpFromDate.compareTo(params["strToDate"])<0){
// thong bao loi
    throw  new Exception("Chỉ được lấy dữ liệu trong vòng 31 ngày");
}

if(params["strFromDate"].compareTo(params["strToDate"])>0){
// thong bao loi
    throw  new Exception("Từ ngày phải nhỏ hơn hoặc bằng đến ngày");
}

// validate dung
true;]]></method>
