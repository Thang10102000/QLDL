            <xml-property name="queryText"><![CDATA[SELECT DECODE (c.unit,  0, 'SP MOBIFONE',  1, 'SP NGOÀI MOBIFONE') unit,
       b.partner_code,
       b.product_name,
       COUNT (b.invoice_order_id) so_gd,
       SUM (b.grand_amount) sotien_gd,
       SUM (DECODE (b.pay_type, 19, b.grand_amount, 0)) sotien_thelienket,
       SUM (DECODE (b.pay_type, 10, b.grand_amount, 0)) sotien_soduvi
FROM summarize_succ a, mobishop b, profile_partner c
WHERE     a.code = c.code
      AND c.TYPE = '0'
      AND a.invoice_order_id = b.invoice_order_id
      AND a.created_time = b.created_time
      AND a.created_time >= ?
      AND a.created_time <= ?
GROUP BY DECODE (c.unit,  0, 'SP MOBIFONE',  1, 'SP NGOÀI MOBIFONE'),
         b.partner_code,
         b.product_name]]></xml-property>
        </oda-data-set>
